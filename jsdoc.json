{
	"plugins": ["plugins/markdown", "node_modules/better-docs/category"],
	"opts": {
		"destination": "./docs/",
		"recurse": true,
		"template": "node_modules/clean-jsdoc-theme",
        "theme_opts": {
            "default_theme": "dark"
        }
	},
	"source": {
		"include": ["src/"]
	},
	"tags": {
		"allowUnknownTags": ["category"]
	},
	"templates": {
		"cleverLinks": true
	}
}
